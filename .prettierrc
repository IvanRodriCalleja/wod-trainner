{
	"useTabs": true,
	"printWidth": 100,
	"tabWidth": 2,
	"singleQuote": true,
	"trailingComma": "none",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"useTabs": false
			}
		}
	],
	"importOrder": [
		"server-only",
		"^(react|react-native)$",
		"<THIRD_PARTY_MODULES>",
		"^@agentero/(.*)$",
		"^(packages|modules|routes)(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderGroupNamespaceSpecifiers": true
}
